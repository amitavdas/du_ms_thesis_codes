    font "Times New Roman,12pt"

set encoding iso_8859_1

set output 'vr.eps'

# set title "BS Potential Profile For Various LFF for Ti"

#: range
#set xr [2 to 6]
#set yr [-0.008 to 0.005]


xr_uf = 2
xr_ut = 6

yr_uf = -0.0075
yr_ut = 0.005

set xr [xr_uf to xr_ut]
set yr [yr_uf to yr_ut]

#: multiplot
set multiplot

#: tics
set format x ""
set xtics 1 nomirror
set mxtics 4

set ytics 0.0025 font "Times New Roman,12pt"
set ytics nomirror
set mytics 2

#: line styles

l_w = 3

set style line 11 dt 3 lw l_w lc "black"
set style line 12 dt 2 lw l_w lc "black"
set style line 13 dt 1 lw l_w lc "black"
set style line 14 dt 4 lw l_w lc "black"
set style line 15 dt 5 lw l_w lc "black"

#: margin
#: l r b t
set margins 1, 0, 0, 0

#: x y
dx = 0.225 
dy = 0.488

s_dx = 0.22
s_dy = 0.4

s_z_dx = 0.15
s_z_dy = 0.2


#: offsets
dx_o = 0.09
dy_o = 0.07

dx_zo = 0.055
dy_zo = 0.17

#: dy offset for bottom plots
dy_o_b = 0.13


#: legends
set key at screen 0.8645, 1.0115 horizontal columns 6

#: labels
unset ylabel
unset xlabel

label_x = 0.85
label_y = 0.15

set origin 0 + dx_o, dy * 1 + dy_o
set size s_dx, s_dy

set label 98 "{/Times-Italic r} (\305)"    right at graph 2.23, -1.30 font "Times New Roman,14"
set label 99 "{/Times-Italic u_{eff}} (r)" right at graph -0.44, 0.15 rotate by 90 font "Times New Roman,14"

set label 1 'Ti' at graph label_x, label_y font "Times New Roman,12"

plot ti_vr_data u 1:5  w l ls 14 title "VS",   \
     ti_vr_data u 1:3  w l ls 12 title "RT",   \
     ti_vr_data u 1:2  w l ls 11 title "IU",   \
     ti_vr_data u 1:6  w l ls 15 title "FD",   \
     ti_vr_data u 1:4  w l ls 13 title "SK",  \

unset key # right bottom

unset label 98
unset label 99

set origin 0 + dx_o + dx_zo, dy * 1 + dy_o + dy_zo
set size s_z_dx, s_z_dy

set xr [2.5 to 4.5 ]
set yr [0.064 to 0.075]

unset label 1

