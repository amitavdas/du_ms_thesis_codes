ti_gr_data = '../data/[01][Ti]/gr.dat'
 v_gr_data = '../data/[02][V]/gr.dat'
cr_gr_data = '../data/[03][Cr]/gr.dat'
mn_gr_data = '../data/[04][Mn]/gr.dat'
fe_gr_data = '../data/[05][Fe]/gr.dat'
co_gr_data = '../data/[06][Co]/gr.dat'
ni_gr_data = '../data/[07][Ni]/gr.dat'
cu_gr_data = '../data/[08][Cu]/gr.dat'

e1_gr_data = '../data/exp/Cu_Fe_Co_Ni.dat'
e2_gr_data = '../data/exp/Ti_V_Cr_Mn.dat'
Cu_geysermans1998 = '../data/exp/Cu.N-body.sim.gr.dat'

#set terminal eps enhanced background rgb 'white' size 2.1, 2.1 font 'Times New Roman,10pt'
set terminal eps enhanced background rgb 'white' \
    size 6, 3.75                                 \
    font "Times New Roman,12pt"

set encoding iso_8859_1

set output 'gr.eps'

# set title "BS Potential Profile For Various LFF for Ti"

#: range
set xr [2 to  10]
set yr [0 to 5.0]

#: multiplot
set multiplot

#: tics
set format x ""
set xtics 3 nomirror
set mxtics 3

set ytics 2 font "Times New Roman,12pt"
set ytics nomirror
set mytics 2

#: Colors

#4878d0
#ee854a
#6acc64
#d65f5f
#956cb4
#8c613c
#dc7ec0
#797979
#d5bb67
#82c6e2

#: line styles

l_w = 1

set style line 11 dt 5       lw l_w lc "black"
set style line 12 dt 2       lw l_w lc "black"
set style line 13 dt 3       lw l_w lc "black"
set style line 14 dt 4       lw l_w lc "black"
set style line 15 dt 1       lw l_w lc "black"
set style line 16 dt '-...-' lw l_w lc "black"

#: point styles
set style line 21 pt 6 ps 0.3 lw 1.5 lc "black"
set style line 22 pt 8 ps 0.5 lw 1   lc "black"

#: margin
set margins 8, 0, 1, 0

#: x y
dx = 0.5
dy = 0.23

#: dy offset
dy_o = 0.07

#: dx offset
dx_o = -0.020

#: dy offset for bottom plots
dy_o_b = 0.05

set origin 0, dy * 3 + dy_o
set size dx, dy

#: legends
set key horizontal maxcols 4 font "Times New Roman,10pt"

#: labels
unset ylabel
unset xlabel

set label 1 'Ti' at graph 0.85, 0.5 font "Times New Roman,12"

plot ti_gr_data u 1:2  w l ls 11 title "IU",   \
     ti_gr_data u 1:3  w l ls 12 title "RT",   \
     ti_gr_data u 1:4  w l ls 13 title "SK",   \
     ti_gr_data u 1:5  w l ls 14 title "VS",   \
     ti_gr_data u 1:6  w l ls 15 title "FD",   \
     ti_gr_data u 7:8  w p ls 21 title "Sim.", \
     e2_gr_data u 1:2  w p ls 22 title "Exp."

set origin 0, dy * 2 + dy_o
set size dx, dy

unset label 1
set label 2 'V' at graph 0.85, 0.5 font "Times New Roman,12"

set yr [0 to 7]

plot  v_gr_data u 1:2  w l ls 11 title "IU",   \
      v_gr_data u 1:3  w l ls 12 title "RT",   \
      v_gr_data u 1:4  w l ls 13 title "SK",   \
      v_gr_data u 1:5  w l ls 14 title "VS",   \
      v_gr_data u 1:6  w l ls 15 title "FD",   \
      v_gr_data u 7:8  w p ls 21 title "Sim.", \
     e2_gr_data u 1:4  w p ls 22 title "Exp."

set origin 0, dy * 1 + dy_o
set size dx, dy

unset label 2
set label 3 'Cr' at graph 0.85, 0.5 font "Times New Roman,12"

set yr [0 to 5]

plot cr_gr_data u 1:2  w l ls 11 title "IU",   \
     cr_gr_data u 1:3  w l ls 12 title "RT",   \
     cr_gr_data u 1:4  w l ls 13 title "SK",   \
     cr_gr_data u 1:5  w l ls 14 title "VS",   \
     cr_gr_data u 1:6  w l ls 15 title "FD",   \
     cr_gr_data u 7:8  w p ls 21 title "Sim.", \
     e2_gr_data u 1:5  w p ls 22 title "Exp."

set xtics font "Times New Roman,12pt"

set format x

set origin 0, dy_o_b
set size dx, dy + 0.02

set bmargin 1.5

unset label 3
set label 4 'Mn' at graph 0.85, 0.5 font "Times New Roman,12"

plot mn_gr_data u 1:2  w l ls 11 title "IU",   \
     mn_gr_data u 1:3  w l ls 12 title "RT",   \
     mn_gr_data u 1:4  w l ls 13 title "SK",   \
     mn_gr_data u 1:5  w l ls 14 title "VS",   \
     mn_gr_data u 1:6  w l ls 15 title "FD",   \
     mn_gr_data u 7:8  w p ls 21 title "Sim.", \
     e2_gr_data u 1:6  w p ls 22 title "Exp."

set format x ""
unset xlabel

set ytics border
# set format y ""
unset ylabel

set origin 0.5, dy * 3 + dy_o
set size dx + dx_o, dy

set lmargin 6
set bmargin 1

unset label 4
set label 5 'Fe' at graph 0.85, 0.5 font "Times New Roman,12"

plot fe_gr_data u 1:2  w l ls 11 title "IU",   \
     fe_gr_data u 1:3  w l ls 12 title "RT",   \
     fe_gr_data u 1:4  w l ls 13 title "SK",   \
     fe_gr_data u 1:5  w l ls 14 title "VS",   \
     fe_gr_data u 1:6  w l ls 15 title "FD",   \
     fe_gr_data u 7:8  w p ls 21 title "Sim.", \
     e1_gr_data u 1:6  w p ls 22 title "Exp."

set origin 0.5, dy * 2 + dy_o
set size dx + dx_o, dy

unset label 5
set label 6 'Co' at graph 0.85, 0.5 font "Times New Roman,12"

plot co_gr_data u 1:2  w l ls 11 title "IU",   \
     co_gr_data u 1:3  w l ls 12 title "RT",   \
     co_gr_data u 1:4  w l ls 13 title "SK",   \
     co_gr_data u 1:5  w l ls 14 title "VS",   \
     co_gr_data u 1:6  w l ls 15 title "FD",   \
     co_gr_data u 7:8  w p ls 21 title "Sim.", \
     e1_gr_data u 1:10 w p ls 22 title "Exp."

set origin 0.5, dy * 1 + dy_o
set size dx + dx_o, dy

unset label 6
set label 7 'Ni' at graph 0.85, 0.5 font "Times New Roman,12"

plot ni_gr_data u 1:2  w l ls 11 title "IU",   \
     ni_gr_data u 1:3  w l ls 12 title "RT",   \
     ni_gr_data u 1:4  w l ls 13 title "SK",   \
     ni_gr_data u 1:5  w l ls 14 title "VS",   \
     ni_gr_data u 1:6  w l ls 15 title "FD",   \
     ni_gr_data u 7:8  w p ls 21 title "Sim.", \
     e1_gr_data u 1:14 w p ls 22 title "Exp."

set xtics font "Times New Roman,12pt"

set origin 0.5, dy_o_b
set size dx + dx_o, dy + 0.02

set format x

set bmargin 1.5

set label 98 "{/Times-Italic r} (\305)"              right at 1.9, -1.6 font "Times New Roman,14"
set label 99 "{/Times-Italic g(r)}" right at -8.0, 11.8 rotate by 90 font "Times New Roman,14"

unset label 7
set label 8 'Cu' at graph 0.85, 0.5 font "Times New Roman,12"

plot Cu_geysermans1998 u 1:2  w l ls 16 title "Ref. Sim.", \
            cu_gr_data u 1:2  w l ls 11 title "IU",   \
            cu_gr_data u 1:3  w l ls 12 title "RT",   \
            cu_gr_data u 1:4  w l ls 13 title "SK",   \
            cu_gr_data u 1:5  w l ls 14 title "VS",   \
            cu_gr_data u 1:6  w l ls 15 title "FD",   \
            cu_gr_data u 7:8  w p ls 21 title "Sim.", \
            e1_gr_data u 1:2  w p ls 22 title "Exp.", \
