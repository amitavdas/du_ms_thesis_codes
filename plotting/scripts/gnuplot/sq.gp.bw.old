set terminal eps enhanced background rgb 'white' \
    size 6, 3.75                                 \
    font "Times New Roman,12pt"

set encoding iso_8859_1

set output 'sq.eps'

#: multiplot
set multiplot

#: tics
set format x ""
set xtics 2 nomirror
set mxtics 2

set ytics 2 font "Times New Roman,12pt"
set ytics nomirror
set mytics 2

#: Colors

#4878d0
#ee854a
#6acc64
#d65f5f
#956cb4
#8c613c
#dc7ec0
#797979
#d5bb67
#82c6e2

#: line styles

l_w = 1

set style line 11 dt 1 lw l_w lc "black"
set style line 12 dt 2 lw l_w lc "black"
set style line 13 dt 3 lw l_w lc "black"
set style line 14 dt 4 lw l_w lc "black"
set style line 15 dt 5 lw l_w lc "black"

#: point styles
set style line 17 pt 8 ps 0.5 lw 1 lc "black"
set style line 16 pt 6 ps 0.3 lw 1 lc "black"

#: margin
set margins 8, 0, 1, 0

#: x y
dx = 0.5
dy = 0.23

#: dy offset
dy_o = 0.07

#: dx offset
dx_o = -0.020

#: dy offset for bottom plots
dy_o_b = 0.05

#: legends
set key horizontal maxcols 3 font "Times New Roman,10"

#: labels
unset ylabel
unset xlabel

#: curve name position

cx = 0.88
cy = 0.4

#: Ti

set origin 0, dy * 3 + dy_o
set size dx, dy

set label 1 'Ti' at graph cx, cy font "Times New Roman,12"

set yr [0 to 6]
set xr [1 to  10]

plot ti_sq_data     u 1:2  w l ls 11 title "IU",   \
     ti_sq_data     u 1:3  w l ls 12 title "RT",   \
     ti_sq_data     u 1:4  w l ls 13 title "SK",   \
     ti_sq_data     u 1:5  w l ls 14 title "VS",   \
     ti_sq_data     u 1:6  w l ls 15 title "FD",   \
     ti_sq_data     u 7:8  w p ls 16 title "Sim.", \
     ti_sq_exp_data u 1:2 every 4 w p ls 17 title "Exp.", \

set origin 0, dy * 2 + dy_o
set size dx, dy

unset label 1
set label 2 'V' at graph cx, cy font "Times New Roman,12"

set xr [1 to  10]
set yr [0 to 8]

